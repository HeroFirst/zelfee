        "catfan/medoo": "v1.1.3",
        "jeremykendall/slim-auth": "2.0.0-alpha",
        "league/oauth2-google": "1.0.1",
        "league/oauth2-facebook": "1.4.4",
        "league/oauth2-client": "1.4.2",
